{
    "name": "fidilly/gendiff",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "fidilly",
            "email": "fidship@hotmail.com"
        }
    ],
    "require": {
	"docopt/docopt": "^1.0",
        "funct/funct": "^1.5",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
	"files": [
        "src/GenDiff.php",
        "src/MakeDiffAst.php",
        "src/DisplayDiffAst.php",
        "src/Renderers/pretty.php",
        "src/Renderers/plain.php"
	],
        "psr-4": {
            "Differ\\Tests\\": "tests"
        }
    },
    "bin": [
	    "bin/gendiff"
    ],
    "scripts": {
        "phpcs": "phpcs",
        "phpcbf": "phpcbf",
        "phpunit": "phpunit",
        "test": "phpunit tests"
    }
}
